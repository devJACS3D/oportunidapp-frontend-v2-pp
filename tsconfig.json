{
	"compileOnSave": false,
	"compilerOptions": {
		"skipLibCheck": true,
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "commonjs",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
			"@styles/*": [
				"./scss/*"
			],
			"@utils/*": [
				"./src/app/utils/*"
			],
			"@apptypes/*": [
				"./src/app/types/*"
			],
			"@entities/*": [
				"./src/app/types/entities/*"
			],
			"@services/*": [
				"./src/app/services/*"
			],
		}
	}
}